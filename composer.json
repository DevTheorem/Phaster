{
    "name": "theodorejb/phaster",
    "description": "A library for quick RESTful APIs",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Theodore Brown",
            "email": "theodorejb@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "psr/http-message": "^1.0",
        "shrikeh/teapot": "^2.3.1",
        "theodorejb/peachy-sql": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "psalm/plugin-phpunit": "^0.16.1",
        "vimeo/psalm": "^4.15"
    },
    "autoload": {
        "psr-4": {"theodorejb\\Phaster\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"theodorejb\\Phaster\\Test\\": "test/src/"}
    }
}
